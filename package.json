    "ky": "^1.14.0",
    "log4js": "^6.9.1",
    "luxon": "^3.7.2",
    "ora": "^9.0.0",
    "p-queue": "^9.0.1",
    "prompts": "^2.4.2",
    "rimraf": "^6.1.2",
    "semver": "^7.7.3",
    "string-width": "^8.1.0",
    "uuid": "^13.0.0",
    "wakachigaki": "^1.3.2",
    "yaml": "^2.8.2",
    "yargs": "^18.0.0"
  }
}