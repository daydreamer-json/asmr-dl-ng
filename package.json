    "@tsconfig/recommended": "^1.0.13",
    "@tsconfig/strictest": "^2.0.8",
    "@types/bun": "latest",
    "@types/cli-progress": "^3.11.6",
    "@types/luxon": "^3.7.1",
    "@types/node": "^24.10.1",
    "@types/prompts": "^2.4.9",
    "@types/semver": "^7.7.1",
    "@types/uuid": "^10.0.0",
    "@types/yargs": "^17.0.35",
    "nodemon": "^3.1.11",
    "parcel": "^2.16.1",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@yutengjing/eld": "^0.0.2",
    "budoux": "^0.7.0",
