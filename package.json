{
  "name": "asmr-dl-ng",
  "version": "1.0.2",
  "description": "ASMR Direct Downloader (New Generation)",
  "author": "daydreamer-json <atarakima1@icloud.com> (https://github.com/daydreamer-json)",
  "license": "AGPL-3.0-or-later",
  "type": "module",
  "module": "dist/main.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daydreamer-json/asmr-dl-ng.git"
  },
  "scripts": {
    "format": "bun x biome format --write src",
    "start": "bun x biome format --write src && tsc && bun run src/prebuild.ts && bun run dist/main.js",
    "build": "bun x biome format --write src && tsc && bun run src/prebuild.ts && bun build --compile --minify --target=bun-windows-x64-baseline dist/main.js --outfile asmr-dl-ng.exe"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@parcel/transformer-typescript-tsc": "^2.16.1",
    "@tsconfig/bun": "^1.0.10",
